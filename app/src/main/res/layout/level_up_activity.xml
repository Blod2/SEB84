<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="ppmint.com.seb_84.LevelUpActivity"
    android:background="#ff0d0d0d">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/stStr"
        android:id="@+id/textView12"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="55dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="10dp"
        android:textColor="#ffffffff"
        android:clickable="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/stInt"
        android:id="@+id/textView13"
        android:layout_below="@+id/textView15"
        android:layout_alignLeft="@+id/textView15"
        android:layout_alignStart="@+id/textView15"
        android:layout_marginTop="20dp"
        android:textColor="#ffffffff"
        android:clickable="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/stAgl"
        android:id="@+id/textView15"
        android:layout_below="@+id/textView12"
        android:layout_alignLeft="@+id/textView12"
        android:layout_alignStart="@+id/textView12"
        android:layout_marginTop="20dp"
        android:textColor="#ffffffff"
        android:clickable="false" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/confirm"
        android:id="@+id/btnConfirm"
        android:layout_marginBottom="16dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/stEnd"
        android:id="@+id/textView14"
        android:layout_below="@+id/textView13"
        android:layout_alignLeft="@+id/textView13"
        android:layout_alignStart="@+id/textView13"
        android:layout_marginTop="20dp"
        android:textColor="#ffffffff"
        android:clickable="false" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etStr"
        android:paddingRight="45dp"
        android:textColor="#ff0006"
        android:clickable="false"
        android:textStyle="bold"
        android:layout_alignTop="@+id/textView12"
        android:layout_centerHorizontal="true"
        android:editable="false" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etAgl"
        android:paddingRight="45dp"
        android:textColor="#ff0006"
        android:clickable="false"
        android:layout_alignTop="@+id/textView15"
        android:layout_alignLeft="@+id/etStr"
        android:layout_alignStart="@+id/etStr"
        android:textStyle="bold"
        android:editable="false" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etInt"
        android:paddingRight="45dp"
        android:textColor="#ff0006"
        android:clickable="false"
        android:layout_alignTop="@+id/textView13"
        android:layout_alignLeft="@+id/etAgl"
        android:layout_alignStart="@+id/etAgl"
        android:textStyle="bold"
        android:editable="false" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/etEnd"
        android:paddingRight="45dp"
        android:textColor="#ff0006"
        android:clickable="false"
        android:layout_alignTop="@+id/textView14"
        android:layout_alignLeft="@+id/etInt"
        android:layout_alignStart="@+id/etInt"
        android:textStyle="bold"
        android:editable="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Tex"
        android:id="@+id/textView16"
        android:layout_marginLeft="32dp"
        android:layout_marginStart="32dp"
        android:layout_alignTop="@+id/btnConfirm"
        android:layout_toRightOf="@+id/etEnd"
        android:layout_toEndOf="@+id/etEnd" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/add"
        android:id="@+id/btnStrAdd"
        android:layout_alignTop="@+id/textView12"
        android:layout_alignLeft="@+id/textView16"
        android:layout_alignStart="@+id/textView16"
        android:layout_alignRight="@+id/textView16"
        android:layout_alignEnd="@+id/textView16"
        android:textStyle="bold"
        android:textSize="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Tex"
        android:id="@+id/textView17"
        android:layout_alignTop="@+id/textView16"
        android:layout_toRightOf="@+id/textView16"
        android:layout_toEndOf="@+id/textView16" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sub"
        android:id="@+id/btnStrSub"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_alignTop="@+id/btnStrAdd"
        android:layout_toRightOf="@+id/btnStrAdd"
        android:layout_alignRight="@+id/btnAglSub"
        android:layout_alignEnd="@+id/btnAglSub" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/add"
        android:id="@+id/btnAglAdd"
        android:layout_alignBottom="@+id/etAgl"
        android:layout_alignLeft="@+id/btnStrAdd"
        android:layout_alignStart="@+id/btnStrAdd"
        android:layout_toLeftOf="@+id/btnStrSub"
        android:layout_toStartOf="@+id/btnStrSub"
        android:textStyle="bold"
        android:textSize="20dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sub"
        android:id="@+id/btnAglSub"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_above="@+id/btnIntAdd"
        android:layout_toRightOf="@+id/btnStrAdd"
        android:layout_alignRight="@+id/btnIntSub"
        android:layout_alignEnd="@+id/btnIntSub" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/add"
        android:id="@+id/btnIntAdd"
        android:layout_above="@+id/btnEndAdd"
        android:layout_alignLeft="@+id/btnAglAdd"
        android:layout_alignStart="@+id/btnAglAdd"
        android:layout_toLeftOf="@+id/btnAglSub"
        android:layout_toStartOf="@+id/btnAglSub"
        android:textStyle="bold"
        android:textSize="20dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/add"
        android:id="@+id/btnEndAdd"
        android:layout_alignBottom="@+id/etEnd"
        android:layout_alignLeft="@+id/textView16"
        android:layout_alignStart="@+id/textView16"
        android:layout_alignRight="@+id/btnIntAdd"
        android:layout_alignEnd="@+id/btnIntAdd"
        android:textStyle="bold"
        android:textSize="20dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sub"
        android:id="@+id/btnIntSub"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_below="@+id/btnAglSub"
        android:layout_alignLeft="@+id/btnEndSub"
        android:layout_alignStart="@+id/btnEndSub"
        android:layout_alignRight="@+id/btnEndSub"
        android:layout_alignEnd="@+id/btnEndSub" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sub"
        android:id="@+id/btnEndSub"
        android:layout_alignBottom="@+id/etEnd"
        android:layout_alignLeft="@+id/textView17"
        android:layout_alignStart="@+id/textView17"
        android:textStyle="bold"
        android:textSize="20dp"
        android:layout_alignRight="@+id/textView17"
        android:layout_alignEnd="@+id/textView17" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/tvStPoints"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/textView15"
        android:textColor="#ff0006" />

</RelativeLayout>
